<div class="wrapper">
  <div class="container mt-5">
    <table class="table table-bordered table-hover table-sm">
      <tr class="text-center">
        <td>案件番号</td>
        <td>依頼主</td>
        <td>タイトル</td>
        <td>依頼受諾日</td>
      </tr>
      <% @replied_orders.each do |order| %>
        <tr class="text-center">
          <td>
            <%= link_to request_path(order.request.id) do %>
              <%= order.request.id %>
            <% end %>
          </td>
          <td><%= order.request.customer.nickname %></td>
          <td><%= order.request.title %></td>
          <td><%= order.created_at %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>