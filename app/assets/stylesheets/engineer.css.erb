/* engineer#index */
.bg-engineer {
  background-image: url("<%= asset_path("sea.jpg") %>") !important;
}
a.btn-radius-solid {
  border: 1px solid #ccc;
  background: #f1e767;
  background: -webkit-gradient(linear, left top, left bottom, from(#fdfbfb), to(#ebedee));
  background: -webkit-linear-gradient(top, #fdfbfb 0%, #ebedee 100%);
  background: linear-gradient(to bottom, #fdfbfb 0%, #ebedee 100%);
  -webkit-box-shadow: inset 1px 1px 1px #fff;
  box-shadow: inset 1px 1px 1px #fff;
}
.book-icon {
  color: #006400;
}

a.btn-radius-solid:hover {
  background: -webkit-gradient(linear, left bottom, left top, from(#fdfbfb), to(#ebedee));
  background: -webkit-linear-gradient(bottom, #fdfbfb 0%, #ebedee 100%);
  background: linear-gradient(to top, #fdfbfb 0%, #ebedee 100%);
}
.engineer-label {
  color: #454839;
  font-size: 16px;
  position: relative;
  padding: 0 0 2px 32px;
}
.engineer-label:before, .engineer-label:after {
  content: "";
  position: absolute;
  bottom: 0;
  width: 12px;
  height: 12px;
  border: 2px solid #005252;
  margin: auto;
}
.engineer-label:before {
  top: -6px;
  left: 0px;
  animation: spin 4s linear infinite;
}
.engineer-label:after {
  top: 6px;
  left: 6px;
  animation: spin 4s linear infinite;
}
@keyframes spin{
  0%{
    transform: rotate(0deg);
  }
  100%{
    transform: rotate(360deg);
  }
}


.engineer-main-btn {
  text-align: center;
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
}
.engineer-important-btn {
  position: relative;
  width: 130px;
  height: 130px;
  box-sizing: border-box;
  text-align: center;
  border-radius: 50%;
  color: black;
  font-weight: bold;
  text-decoration: none;
  outline: none;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
}
.engineer-important-btn:after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #AAAD9B;
  width: 85%;
  height: 85%;
  border: 2px solid white;
  border-radius: 50%;
  transform: translate(-50%, -50%) scale(1.1);
  transition: .3s ease;
  z-index: -1;
}
.engineer-important-btn:hover:after {
  transform: translate(-50%, -50%) scale(1);
  border-color: #fff;
  background-color: #AAAD9B;
  z-index: -1;
}

.engineer-normal-btn {
  position: relative;
  width: 130px;
  height: 130px;
  box-sizing: border-box;
  text-align: center;
  border-radius: 50%;
  color: black;
  font-weight: bold;
  text-decoration: none;
  outline: none;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
}
.engineer-normal-btn:after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  background-color: #AAAD9B;
  width: 85%;
  height: 85%;
  border: 2px solid white;
  border-radius: 50%;
  transform: translate(-50%, -50%) scale(1.1);
  transition: .3s ease;
  z-index: -1;
}
.engineer-normal-btn:hover:after {
  transform: translate(-50%, -50%) scale(1);
  border-color: #fff;
  background-color: #AAAD9B;
  z-index: -1;
}
hr {
  color: #AAAD9B;
}


.engineer-menu {
  text-align: center;
  display: grid;
  grid-template-columns: 1fr 1fr;
}
.engineer-sub-btn {
  width: 98%;
  display: inline-block;
  position: relative;
  padding: 0.5em 1em;
  text-decoration: none !important;
  color: #454839;
  transition: .4s;
}
.engineer-sub-btn:hover {
  background: #AAAD9B;
}
.engineer-sub-btn:before, .engineer-sub-btn:after {
  position: absolute;
  top: 0;
  content:'';
  width: 8px;
  height: 100%;
  display: inline-block;
}
.engineer-sub-btn:before {
  border-left: solid 1px #454839;
  border-top: solid 1px #454839;
  border-bottom: solid 1px #454839;
  left: 0;
}
.engineer-sub-btn:after {
  content: '';
  border-top: solid 1px #454839;
  border-right: solid 1px #454839;
  border-bottom: solid 1px #454839;
  right: 0;
}

